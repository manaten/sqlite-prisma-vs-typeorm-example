{
  "name": "ts-node-cli-template",
  "author": "manaten@manaten.net",
  "license": "MIT",
  "type": "module",
  "version": "1.0.0",
  "description": "manaten's ts-node cli script template.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node --esm --experimentalSpecifierResolution node src/index.ts",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "lint-staged": "lint-staged -c .lintstagedrc",
    "fix": "eslint --fix ./**/*.{js,jsx,ts,tsx}",
    "prettier": "prettier --write --ignore-unknown ."
  },
  "dependencies": {
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eclint": "^2.8.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
